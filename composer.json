{
  "name": "erykai/erykia",
  "description": "Artificial intelligence for creating websites, systems in php with restfull api",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "alexdeovidal",
      "email": "webav.com.br@gmail.com"
    }
  ],
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "Source\\": "app/",
      "Modules\\": "modules/"
    },
    "files": [
      "config/_config.php"
    ]
  },
  "require": {
    "erykai/routes": "v1.3.5",
    "erykai/database": "v1.4.4",
    "erykai/upload": "v1.4.2",
    "erykai/request": "v1.3.1",
    "erykai/response": "v1.1.0",
    "erykai/migration": "v1.2.0",
    "erykai/translate": "v2.2.1",
    "erykai/template": "v1.1.0",
    "erykai/pluralize": "v1.1.0",
    "erykai/cryption": "v2.1.3",
    "erykai/compress": "v1.0.2",
    "ext-fileinfo": "*",
    "php": ">=8.0",
    "ext-pdo": "*",
    "ext-curl": "*",
    "ext-imagick": "*",
    "ext-gd": "*"
  }
}