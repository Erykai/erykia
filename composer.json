{
  "name": "erykai/erykia",
  "description": "Artificial intelligence for creating websites, systems in php with restfull api",
  "minimum-stability": "stable",
  "license": "MIT",
  "scripts": {
    "startMac": "sh start.sh",
    "startWin": "powershell .\\start.ps1",
    "startLinux": "sh start.sh"
  },
  "authors": [
    {
      "name": "alexdeovidal",
      "email": "webav.com.br@gmail.com"
    }
  ],
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "Source\\": "app/"
    },
    "files": [
      "config/_config.php"
    ]
  },
  "require": {
    "erykai/routes": "v1.2.0",
    "erykai/database": "v1.3.0",
    "erykai/upload": "v1.2.2",
    "erykai/request": "v1.2.1",
    "erykai/response": "v1.0.1",
    "erykai/migration": "v1.1.0",
    "erykai/translate": "v2.1.1",
    "erykai/pluralize": "v1.0.1",
    "ext-fileinfo": "*",
    "php": ">=8.0",
    "ext-pdo": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "alexdeovidal/docker-dev": "main-dev"
  }
}